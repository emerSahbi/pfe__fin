            "type": "attack-pattern",
            "name": "Dynamic-link Library Injection",
            "x_mitre_data_sources": [
                "Process: Process Modification",
                "Module: Module Load",
                "Process: OS API Execution",
                "Process: Process Access"
            ],
            "x_mitre_version": "1.1",
